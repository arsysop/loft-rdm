<plugin>
   <extension-point id="revisions" name="%extension.point.name" schema="schema/revisions.exsd"/>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated spec -->
      <package
            uri="http://www.arsysop.ru/rgm/spec/0.2.0"
            class="ru.arsysop.loft.rgm.spec.model.meta.SpecPackage"
            genModel="model/spec.genmodel"/>
   </extension>

   <extension point="org.eclipse.emf.ecore.extension_parser">
      <!-- @generated spec -->
      <parser
            type="spec"
            class="ru.arsysop.loft.rgm.spec.model.util.SpecResourceFactoryImpl"/>
   </extension>
   <extension
         point="ru.arsysop.loft.rgm.spec.model.revisions">
      <revision
            class="ru.arsysop.loft.rgm.spec.model.Cpp14"
            id="ru.arsysop.loft.rgm.spec.model.cpp14">
      </revision>
      <revision
            class="ru.arsysop.loft.rgm.spec.model.Cpp17"
            id="ru.arsysop.loft.rgm.spec.model.cpp17">
      </revision>
   </extension>
</plugin>
