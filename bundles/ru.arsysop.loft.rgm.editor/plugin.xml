<plugin>

   <extension
         point="org.eclipse.core.runtime.applications"
         id="RgmEditorAdvisorApplication">
      <!-- @generated rgm -->
      <application>
         <run class="ru.arsysop.loft.rgm.editor.RgmEditorAdvisor$Application"/>
      </application>
   </extension>

   <extension point="org.eclipse.ui.perspectives">
      <!-- @generated rgm -->
      <perspective
            name="%_UI_Perspective_label"
            class="ru.arsysop.loft.rgm.editor.RgmEditorAdvisor$Perspective"
            id="ru.arsysop.loft.rgm.editor.RgmEditorAdvisorPerspective">
      </perspective>
   </extension>

   <extension point="org.eclipse.ui.commands">
      <!-- @generated rgm -->
      <command
            name="%_UI_Menu_OpenURI_label"
            description="%_UI_Menu_OpenURI_description"
            categoryId="org.eclipse.ui.category.file"
            id="ru.arsysop.loft.rgm.editor.RgmEditorAdvisorOpenURICommand"/>
      <command
            name="%_UI_Menu_Open_label"
            description="%_UI_Menu_Open_description"
            categoryId="org.eclipse.ui.category.file"
            id="ru.arsysop.loft.rgm.editor.RgmEditorAdvisorOpenCommand"/>
   </extension>

   <extension point="org.eclipse.ui.bindings">
      <!-- @generated rgm -->
      <key
            commandId="ru.arsysop.loft.rgm.editor.RgmEditorAdvisorOpenURICommand"
            sequence="M1+U"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"/>
      <key
            commandId="ru.arsysop.loft.rgm.editor.RgmEditorAdvisorOpenCommand"
            sequence="M1+O"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"/>
   </extension>

   <extension point="org.eclipse.ui.actionSets">
      <!-- @generated rgm -->
      <actionSet
            label="%_UI_RgmEditorAdvisor_ActionSet_label"
            visible="true"
            id="RgmEditorAdvisorActionSet">
         <action
               label="%_UI_Menu_About_label"
               class="ru.arsysop.loft.rgm.editor.RgmEditorAdvisor$AboutAction"
               menubarPath="help/additions"
               id="ru.arsysop.loft.rgm.editor.RgmEditorAdvisorAboutAction"/>
         <action
               label="%_UI_Menu_OpenURI_label"
               definitionId="ru.arsysop.loft.rgm.editor.RgmEditorAdvisorOpenURICommand"
               class="ru.arsysop.loft.rgm.editor.RgmEditorAdvisor$OpenURIAction"
               menubarPath="file/additions"
               id="ru.arsysop.loft.rgm.editor.RgmEditorAdvisorOpenURIAction"/>
         <action
               label="%_UI_Menu_Open_label"
               definitionId="ru.arsysop.loft.rgm.editor.RgmEditorAdvisorOpenCommand"
               class="ru.arsysop.loft.rgm.editor.RgmEditorAdvisor$OpenAction"
               menubarPath="file/additions"
               id="ru.arsysop.loft.rgm.editor.RgmEditorAdvisorOpenAction"/>
      </actionSet>
   </extension>

   <extension point="org.eclipse.ui.actionSets">
      <!-- @generated rgm -->
      <actionSet
            label="%_UI_RgmModelWizard_ActionSet_label"
            visible="true"
            id="ru.arsysop.loft.rgm.editor.RgmActionBarContributorActionSet">
         <action
               label="%_UI_RgmModelWizard_label"
               class="ru.arsysop.loft.rgm.editor.RgmActionBarContributor$NewAction"
               menubarPath="file/new/additions"
               id="ru.arsysop.loft.rgm.editor.RgmActionBarContributorNewAction"/>
      </actionSet>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated rgm -->
      <editor
            id="ru.arsysop.loft.rgm.editor.RgmEditorID"
            name="%_UI_RgmEditor_label"
            icon="icons/full/obj16/RgmModelFile.gif"
            extensions="rgm"
            class="ru.arsysop.loft.rgm.editor.RgmEditor"
            contributorClass="ru.arsysop.loft.rgm.editor.RgmActionBarContributor">
      </editor>
   </extension>

</plugin>
